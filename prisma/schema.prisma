generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["relationJoins"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Organization {
  id           String       @id @default(cuid())
  name         String       @unique
  orgLinkName  String?      @unique
  createdAt    DateTime     @default(now())
  updatedAt    DateTime     @updatedAt
  phone        String       @unique
  dispensaries Dispensary[]
  Supplier     Supplier[]
}

model Dispensary {
  id                        String                   @id @default(cuid())
  organizationId            String
  organization              Organization             @relation(fields: [organizationId], references: [id])
  name                      String
  storeLinkName             String?                  @unique
  cannabisLicense           String                   @unique @default("")
  cannabisLicenseExpireDate String                   @default("")
  businessLicense           String?                  @unique
  phone                     String                   @unique
  email                     String                   @unique
  locationAddress           String
  locationCity              String
  locationZipCode           String
  createdAt                 DateTime                 @default(now())
  updatedAt                 DateTime                 @updatedAt
  dispensaryType            DispensaryType
  metrcApiKey               String?                  @unique
  metrcConnectionStatus     Boolean                  @default(false)
  metrcLicenseNumber        String?                  @unique
  locationState             StateType
  isActive                  Boolean                  @default(false)
  isCustomerAgeVerify       Boolean                  @default(false)
  customerAgeLimit          Int                      @default(0)
  storeTimeZone             Timezones                @default(EST)
  smsOrderStart             String?
  smsOrderEnd               String?
  users                     User[]
  Product                   Product[]
  Customer                  Customer[]
  Loyalty                   Loyalty[]
  Discount                  Discount[]
  TaxSetting                TaxSetting[]
  ItemCategory              ItemCategory[]
  MetrcCategory             MetrcItemCategory[]
  Order                     Order[]
  Package                   Package[]
  Drawer                    Drawer[]
  AssignPackage             AssignPackage[]
  Transfer                  Transfer[]
  DeliveryPackages          DeliveryPackages[]
  TaxHistory                TaxHistory[]
  TaxApply                  TaxApply[]
  DiscountHistory           DiscountHistory[]
  CustomerQueue             CustomerQueue[]
  LoyaltyHistory            LoyaltyHistory[]
  SyncHistory               SyncHistory[]
  PurchaseLimit             PurchaseLimit[]
  AdjustPackage             AdjustPackage[]
  MetrcAdjustmentReasons    MetrcAdjustmentReasons[]
  TestResult                TestResult[]
  PrintSetting              PrintSetting[]
  ActionHistory             ActionHistory[]

  @@unique([organizationId, name])
}

model User {
  id                  String          @id @default(cuid())
  dispensaryId        String
  dispensary          Dispensary      @relation(fields: [dispensaryId], references: [id])
  email               String
  name                String
  createdAt           DateTime        @default(now())
  updatedAt           DateTime        @updatedAt
  userType            UserType
  isActive            Boolean         @default(false)
  isDispensaryAdmin   Boolean         @default(false)
  isEmailVerified     Boolean         @default(false)
  isOrganizationAdmin Boolean         @default(false)
  phone               String
  password            String
  Product             Product[]
  Order               Order[]
  Drawer              Drawer[]
  AssignPackage       AssignPackage[]
  Transfer            Transfer[]
  CustomerQueue       CustomerQueue[]
  SyncHistory         SyncHistory[]
  ActionHistory       ActionHistory[]

  @@unique([dispensaryId, name])
  @@unique([dispensaryId, email])
}

model Customer {
  id                           String          @id @default(cuid())
  name                         String
  birthday                     String
  email                        String
  phone                        String
  dispensaryId                 String
  dispensary                   Dispensary      @relation(fields: [dispensaryId], references: [id])
  isActive                     Boolean
  driverLicense                String
  driverLicenseExpirationDate  String
  isMedical                    Boolean
  medicalLicense               String
  medicalLicenseExpirationDate String
  address                      String?         @default("")
  city                         String?
  usState                      String?
  zipCode                      String?
  loyaltyPoints                Float?          @default(0)
  status                       CustomerStatus
  isTaxExempt                  Boolean         @default(false)
  note                         String          @default("")
  // discount                     Discount?      @relation(fields: [status], references: [applyTarget])
  createdAt                    DateTime        @default(now())
  updatedAt                    DateTime        @updatedAt
  MFType                       MFType          @default(MALE)
  Order                        Order[]
  CustomerQueue                CustomerQueue[]
  ActionHistory                ActionHistory[]

  @@unique([dispensaryId, medicalLicense])
}

model CustomerQueue {
  id           String     @id @default(cuid())
  dispensaryId String
  dispensary   Dispensary @relation(fields: [dispensaryId], references: [id])
  customerId   String
  customer     Customer   @relation(fields: [customerId], references: [id])
  userId       String
  user         User       @relation(fields: [userId], references: [id])
  createdAt    DateTime   @default(now())
  updatedAt    DateTime   @updatedAt

  @@unique([dispensaryId, customerId])
}

model SyncHistory {
  id           String     @id @default(cuid())
  dispensaryId String
  dispensary   Dispensary @relation(fields: [dispensaryId], references: [id])
  userId       String
  user         User       @relation(fields: [userId], references: [id])
  syncType     SyncType
  isSuccess    Boolean    @default(false)
  createdAt    DateTime   @default(now())
  updatedAt    DateTime   @updatedAt
}

model Supplier {
  id              String        @id @default(cuid())
  organizationId  String
  organization    Organization  @relation(fields: [organizationId], references: [id])
  name            String?
  businessLicense String?
  UBI             String?
  phone           String?
  email           String?
  locationAddress String?
  locationCity    String?
  locationZipCode String?
  supplierType    SupplierType?
  isActive        Boolean
  createdAt       DateTime      @default(now())
  updatedAt       DateTime      @updatedAt
  locationState   StateType?
  Product         Product[]
  Transfer        Transfer[]

  @@unique([organizationId, businessLicense])
}

model Order {
  id                Int               @id @default(autoincrement())
  metrcId           Int?              @unique
  dispensaryId      String
  dispensary        Dispensary        @relation(fields: [dispensaryId], references: [id])
  drawerId          String
  drawer            Drawer            @relation(fields: [drawerId], references: [id])
  userId            String
  user              User              @relation(fields: [userId], references: [id])
  customerId        String?
  customer          Customer?         @relation(fields: [customerId], references: [id])
  cannabisLicense   String?
  orderType         OrderType         @default(SALE)
  description       String?
  cost              Float?            @default(0)
  cashAmount        Float?            @default(0)
  changeDue         Float?            @default(0)
  otherAmount       Float?            @default(0)
  loyalty           Float?            @default(0)
  tax               Float?            @default(0)
  discount          Float?            @default(0)
  mjType            OrderMjType       @default(NMJ)
  status            OrderStatus
  orderDate         String?
  isReportedToMetrc Boolean           @default(false)
  originalOrder     Int?              @default(0)
  returnReason      String?
  voidReason        String?
  voidedAt          DateTime?
  completedAt       DateTime?
  salesDateTime     String?
  createdAt         DateTime          @default(now())
  updatedAt         DateTime          @updatedAt
  OrderItem         OrderItem[]
  TaxHistory        TaxHistory[]
  DiscountHistory   DiscountHistory[]
  LoyaltyHistory    LoyaltyHistory[]
  ActionHistory     ActionHistory[]
}

model OrderItem {
  id                 String         @id @default(cuid())
  orderId            Int
  order              Order          @relation(fields: [orderId], references: [id])
  productId          String?
  product            Product?       @relation(fields: [productId], references: [id])
  packageLabel       String?
  package            Package?       @relation(fields: [packageLabel], references: [packageLabel], map: "fk_OrderItem_packageLabel_package")
  assignPackage      AssignPackage? @relation(fields: [packageLabel], references: [packageLabel], map: "fk_OrderItem_packageLabel_assignPackage")
  quantity           Float?
  unitWeight         Float?
  isReturned         Boolean        @default(false)
  isRestockForReturn Boolean        @default(false)
  price              Float?         @default(0)
  cost               Float?         @default(0)
  amount             Float?         @default(0)
  costAmount         Float?         @default(0)
  discountedAmount   Float?         @default(0)
  loyaltyAmount      Float?         @default(0)
  metrcItemName      String?
  mjType             OrderMjType    @default(NMJ)
  createdAt          DateTime       @default(now())
  updatedAt          DateTime       @updatedAt
  TaxHistory         TaxHistory[]
}

model Loyalty {
  id               String      @id @default(cuid())
  dispensaryId     String
  dispensary       Dispensary  @relation(fields: [dispensaryId], references: [id])
  name             String
  type             LoyaltyType
  pointWorth       Float?
  applyDurationSet Boolean
  applyFrom        String
  applyTo          String
  createdAt        DateTime    @default(now())
  updatedAt        DateTime    @updatedAt
  color            String
  isActive         Boolean
  isAdminPin       Boolean

  @@unique([dispensaryId, name])
  @@unique([dispensaryId, type])
}

model Discount {
  id                         String         @id @default(cuid())
  dispensaryId               String
  dispensary                 Dispensary     @relation(fields: [dispensaryId], references: [id])
  applyTarget                CustomerStatus
  name                       String
  type                       DiscountType
  applyDurationSet           Boolean
  applyFrom                  String
  applyTo                    String
  isActive                   Boolean
  isAdminPin                 Boolean
  isEnterManualAmount        Boolean        @default(false)
  isLimitManualAmount        Boolean        @default(false)
  manualDiscountLimitPercent Float?         @default(0)
  color                      String
  createdAt                  DateTime       @default(now())
  updatedAt                  DateTime       @updatedAt
  discountPercent            Float?         @default(0)

  @@unique([dispensaryId, name])
  @@unique([dispensaryId, type])
}

model DiscountHistory {
  id             String         @id @default(cuid())
  dispensaryId   String
  dispensary     Dispensary     @relation(fields: [dispensaryId], references: [id])
  orderId        Int
  order          Order          @relation(fields: [orderId], references: [id])
  discountName   String
  discountMethod DiscountMethod
  value          Float?         @default(0)
}

model LoyaltyHistory {
  id           String        @id @default(cuid())
  txType       LoyaltyTxType @default(spend)
  dispensaryId String
  dispensary   Dispensary    @relation(fields: [dispensaryId], references: [id])
  orderId      Int
  order        Order         @relation(fields: [orderId], references: [id])
  loyaltyName  String
  loyaltyType  LoyaltyType
  loyaltyWorth Float?        @default(0)
  value        Float?        @default(0)
}

model ItemCategory {
  id                String             @id @default(cuid())
  dispensaryId      String
  dispensary        Dispensary         @relation(fields: [dispensaryId], references: [id])
  metrcCategory     String?
  name              String
  color             String             @default("#03a9f4")
  containMj         Boolean            @default(false)
  purchaseLimitType PurchaseLimitType?
  createdAt         DateTime           @default(now())
  updatedAt         DateTime           @updatedAt
  Product           Product[]

  @@unique([dispensaryId, name])
}

model PurchaseLimit {
  id                  String              @id @default(cuid())
  dispensaryId        String
  dispensary          Dispensary          @relation(fields: [dispensaryId], references: [id])
  purchaseLimitMethod PurchaseLimitMethod
  purchaseLimitType   PurchaseLimitType
  purchaseLimitAmount Float?
  limitUnit           UnitOfMeasure
  limitWeight         LimitWeight
  createdAt           DateTime            @default(now())
  updatedAt           DateTime            @updatedAt

  @@unique([dispensaryId, purchaseLimitType])
}

model TaxSetting {
  id                        String            @id @default(cuid())
  dispensaryId              String
  dispensary                Dispensary        @relation(fields: [dispensaryId], references: [id])
  name                      String
  rate                      Float?
  applyTo                   TaxSettingApplyTo
  compoundTaxes             String[]
  isExcludeFromRecreational Boolean           @default(false)
  isExcludeFromTaxExempt    Boolean           @default(false)
  createdAt                 DateTime          @default(now())
  updatedAt                 DateTime          @updatedAt
  categories                Json[]

  @@unique([dispensaryId, name])
}

model TaxHistory {
  id              String     @id @default(cuid())
  dispensaryId    String
  dispensary      Dispensary @relation(fields: [dispensaryId], references: [id])
  orderId         Int
  order           Order      @relation(fields: [orderId], references: [id])
  orderItemId     String
  orderItem       OrderItem  @relation(fields: [orderItemId], references: [id])
  taxName         String
  taxPercent      Float?     @default(0)
  compoundPercent Float?     @default(0)
  taxAmount       Float?
  createdAt       DateTime   @default(now())
  updatedAt       DateTime   @updatedAt

  @@unique([orderItemId, taxName])
}

model TaxApply {
  id              String            @id @default(cuid())
  dispensaryId    String
  dispensary      Dispensary        @relation(fields: [dispensaryId], references: [id])
  applyTo         TaxSettingApplyTo @default(MJ)
  taxName         String
  basePercent     Float?            @default(0)
  compoundPercent Float?            @default(0)
  isTaxExempt     Boolean           @default(false)
  createdAt       DateTime          @default(now())
  updatedAt       DateTime          @updatedAt
}

model AdjustPackage {
  id            String     @id @default(cuid())
  dispensaryId  String
  dispensary    Dispensary @relation(fields: [dispensaryId], references: [id])
  packageLabel  String
  package       Package    @relation(fields: [packageLabel], references: [packageLabel])
  newQty        Float?
  deltaQty      Float?     @default(0)
  reason        String
  notes         String
  needMetrcSync Boolean    @default(false)
  syncMetrc     Boolean    @default(false)
  createdAt     DateTime   @default(now())
  updatedAt     DateTime   @updatedAt
}

model Drawer {
  id                String       @id @default(cuid())
  dispensaryId      String
  dispensary        Dispensary   @relation(fields: [dispensaryId], references: [id])
  userId            String
  user              User         @relation(fields: [userId], references: [id])
  startAmount       Float?
  endAmount         Float?
  totalDeposite     Float?       @default(0)
  note              String
  comment           String?
  status            DrawerStatus
  register          String
  discrepancyReason String?
  isUsing           Boolean      @default(false)
  endedAt           DateTime?
  createdAt         DateTime     @default(now())
  updatedAt         DateTime     @updatedAt
  Order             Order[]
  MoneyDrop         MoneyDrop[]
}

model MoneyDrop {
  id        String   @id @default(cuid())
  drawerId  String
  drawer    Drawer   @relation(fields: [drawerId], references: [id])
  dropType  DropType
  amount    Float?   @default(0)
  reason    String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model Transfer {
  id                           String             @id @default(cuid())
  dispensaryId                 String
  dispensary                   Dispensary         @relation(fields: [dispensaryId], references: [id])
  userId                       String
  user                         User               @relation(fields: [userId], references: [id])
  transferType                 TransferType
  isMJ                         Boolean
  status                       TransferStatus
  supplierId                   String?
  supplier                     Supplier?          @relation(fields: [supplierId], references: [id])
  //Metrc indexes start
  transferId                   Int?               @unique
  deliveryId                   Int?               @unique
  PackageCount                 Int?               @default(0)
  ReceivedPackageCount         Int?               @default(0)
  assignedPackageCount         Int?               @default(0)
  CreatedDateTime              String?
  ReceivedDateTime             String?
  ShipperFacilityLicenseNumber String?
  ShipperFacilityName          String?
  //Metrc indexes end
  createdAt                    DateTime           @default(now())
  updatedAt                    DateTime           @updatedAt
  Package                      Package[]
  AssignPackage                AssignPackage[]
  DeliveryPackages             DeliveryPackages[]
}

model Product {
  id                     String               @id @default(cuid())
  dispensaryId           String
  dispensary             Dispensary           @relation(fields: [dispensaryId], references: [id])
  userId                 String
  user                   User                 @relation(fields: [userId], references: [id])
  supplierId             String
  supplier               Supplier             @relation(fields: [supplierId], references: [id])
  itemCategoryId         String
  itemCategory           ItemCategory         @relation(fields: [itemCategoryId], references: [id])
  name                   String
  sku                    String?
  upc                    String?
  price                  Float?
  unitOfMeasure          UnitOfMeasure?
  productUnitOfMeasure   ProductUnitOfMeasure @default(ea)
  unitOfUnitWeight       UnitOfMeasure?
  unitOfNetWeight        UnitOfMeasure?
  unitWeight             Float?
  isApplyUnitWeight      Boolean              @default(false)
  netWeight              Float?
  isConnectedWithPackage Boolean              @default(false)
  otherPosUniqueId       String?
  createdAt              DateTime             @default(now())
  updatedAt              DateTime             @updatedAt
  Package                Package[]
  AssignPackage          AssignPackage[]
  OrderItem              OrderItem[]
  ActionHistory          ActionHistory[]

  @@unique([dispensaryId, name, price, productUnitOfMeasure, unitWeight, netWeight])
}

model PrintSetting {
  id              String     @id @default(cuid())
  dispensaryId    String
  dispensary      Dispensary @relation(fields: [dispensaryId], references: [id])
  printType       PrintType
  isEnabled       Boolean
  dimensionWidth  Float?
  dimensionHeight Float?
  marginTop       Float?
  marginBottom    Float?
  marginLeft      Float?
  marginRight     Float?
  fontSize        Float?
  topText         String?
  bottomText      String?
  createdAt       DateTime   @default(now())
  updatedAt       DateTime   @updatedAt

  @@unique([dispensaryId, printType])
}

model MetrcAdjustmentReasons {
  id                          String     @id @default(cuid())
  dispensaryId                String
  dispensary                  Dispensary @relation(fields: [dispensaryId], references: [id])
  Name                        String
  RequiresNote                Boolean
  RequiresWasteWeight         Boolean
  RequiresImmatureWasteWeight Boolean
  RequiresMatureWasteWeight   Boolean

  @@unique([dispensaryId, Name])
}

model TestResult {
  id                          String     @id @default(cuid())
  dispensaryId                String
  dispensary                  Dispensary @relation(fields: [dispensaryId], references: [id])
  packageId                   Int
  package                     Package    @relation(fields: [packageId], references: [packageId])
  labTestResultId             Int?
  labFacilityLicenseNumber    String?
  sourcePackageLabel          String?
  testPerformedDate           String
  labTestResultDocumentFileId Int?
  testTypeName                String
  testPassed                  Boolean
  testResultLevel             Float
  testComment                 String?
  createdAt                   DateTime   @default(now())
  updatedAt                   DateTime   @updatedAt

  @@unique([packageId, testTypeName])
}

model MetrcItemCategory {
  id                                String     @id @default(cuid())
  dispensaryId                      String
  dispensary                        Dispensary @relation(fields: [dispensaryId], references: [id])
  Name                              String
  ProductCategoryType               String
  QuantityType                      String
  RequiresStrain                    Boolean
  RequiresItemBrand                 Boolean
  RequiresAdministrationMethod      Boolean
  RequiresUnitCbdPercent            Boolean
  RequiresUnitCbdContent            Boolean
  RequiresUnitCbdContentDose        Boolean
  RequiresUnitThcPercent            Boolean
  RequiresUnitThcContent            Boolean
  RequiresUnitThcContentDose        Boolean
  RequiresUnitVolume                Boolean
  RequiresUnitWeight                Boolean
  RequiresServingSize               Boolean
  RequiresSupplyDurationDays        Boolean
  RequiresNumberOfDoses             Boolean
  RequiresPublicIngredients         Boolean
  RequiresDescription               Boolean
  RequiresAllergens                 Boolean
  RequiresProductPhotos             Int
  RequiresProductPhotoDescription   Boolean
  RequiresLabelPhotos               Int
  RequiresLabelPhotoDescription     Boolean
  RequiresPackagingPhotos           Int
  RequiresPackagingPhotoDescription Boolean
  CanContainSeeds                   Boolean
  CanBeRemediated                   Boolean
  CanBeDestroyed                    Boolean
  CanBeDecontaminated               Boolean?
  RequiresProductPDFDocuments       Int
  LabTestBatchNames                 String[]
  createdAt                         DateTime   @default(now())
  updatedAt                         DateTime   @updatedAt

  @@unique([dispensaryId, Name])
}

model ActionHistory {
  id             String         @id @default(cuid())
  dispensaryId   String
  dispensary     Dispensary     @relation(fields: [dispensaryId], references: [id])
  userId         String
  user           User           @relation(fields: [userId], references: [id])
  userName       String
  actionName     ActionNameList
  packageLabel   String?
  package        Package?       @relation(fields: [packageLabel], references: [packageLabel])
  orderId        Int?
  order          Order?         @relation(fields: [orderId], references: [id])
  productId      String?
  product        Product?       @relation(fields: [productId], references: [id])
  customerId     String?
  customer       Customer?      @relation(fields: [customerId], references: [id])
  targetRecordId String?
  f1             String?
  f2             String?
  f3             String?
  f4             String?
  createdAt      DateTime       @default(now())
  updatedAt      DateTime       @updatedAt
}

model AssignPackage {
  id              String      @id @default(cuid())
  dispensaryId    String
  dispensary      Dispensary  @relation(fields: [dispensaryId], references: [id])
  userId          String
  user            User        @relation(fields: [userId], references: [id])
  transferId      String?
  transfer        Transfer?   @relation(fields: [transferId], references: [id])
  productId       String?
  product         Product?    @relation(fields: [productId], references: [id])
  packageLabel    String?     @unique
  package         Package?    @relation(fields: [packageLabel], references: [packageLabel])
  originalQty     Float?      @default(0)
  posQty          Float?      @default(0)
  cost            Float?      @default(0)
  createdAt       DateTime    @default(now())
  updatedAt       DateTime    @updatedAt
  metrcTransferId Int?
  OrderItem       OrderItem[]
}

model Package {
  id                                      String            @id @default(cuid())
  dispensaryId                            String
  dispensary                              Dispensary        @relation(fields: [dispensaryId], references: [id])
  nonMjTransferId                         String?
  nonMjTransfer                           Transfer?         @relation(fields: [nonMjTransferId], references: [id])
  assignPackageId                         String?           @unique
  packageId                               Int?              @unique
  migrationIdForNonMjPackage              String?           @unique
  delivery                                DeliveryPackages?
  packageStatus                           PackageStatus     @default(PENDING)
  productId                               String?
  product                                 Product?          @relation(fields: [productId], references: [id])
  cost                                    Float?            @default(0)
  originalQty                             Float?            @default(0)
  Quantity                                Float?            @default(0)
  posQty                                  Float?            @default(0)
  isConnectedWithProduct                  Boolean?          @default(false)
  packageLabel                            String?           @unique
  assignPackage                           AssignPackage?
  PackageType                             String?           @default("Product")
  SourceHarvestCount                      Int?
  SourcePackageCount                      Int?
  SourceProcessingJobCount                Int?
  SourceHarvestNames                      String?
  SourcePackageLabels                     String?
  LocationId                              Int?
  LocationName                            String?
  LocationTypeName                        String?
  UnitOfMeasureName                       String?
  UnitOfMeasureAbbreviation               String?
  PatientLicenseNumber                    String?
  ItemFromFacilityLicenseNumber           String?
  ItemFromFacilityName                    String?
  Note                                    String?
  PackagedDate                            String?
  ExpirationDate                          String?
  SellByDate                              String?
  UseByDate                               String?
  InitialLabTestingState                  String?
  LabTestingState                         String?
  LabTestingStateDate                     String?
  LabTestingPerformedDate                 String?
  LabTestResultExpirationDateTime         String?
  LabTestingRecordedDate                  String?
  IsProductionBatch                       Boolean?
  ProductionBatchNumber                   String?
  SourceProductionBatchNumbers            String?
  IsTradeSample                           Boolean?
  IsTradeSamplePersistent                 Boolean?
  SourcePackageIsTradeSample              Boolean?
  IsDonation                              Boolean?
  IsDonationPersistent                    Boolean?
  SourcePackageIsDonation                 Boolean?
  IsTestingSample                         Boolean?
  IsProcessValidationTestingSample        Boolean?
  ProductRequiresRemediation              Boolean?
  ContainsRemediatedProduct               Boolean?
  RemediationDate                         String?
  ProductRequiresDecontamination          Boolean?
  ContainsDecontaminatedProduct           Boolean?
  DecontaminationDate                     Boolean?
  ReceivedDateTime                        String?
  ReceivedFromManifestNumber              String?
  ReceivedFromFacilityLicenseNumber       String?
  ReceivedFromFacilityName                String?
  IsOnHold                                Boolean?
  IsOnRecall                              Boolean?
  ArchivedDate                            String?
  IsFinished                              Boolean?
  FinishedDate                            String?
  IsOnTrip                                Boolean?
  IsOnRetailerDelivery                    Boolean?
  PackageForProductDestruction            String?
  LastModified                            String?
  RetailIdQrCount                         Int?
  itemId                                  Int?
  itemName                                String?
  itemProductCategoryName                 String?
  itemProductCategoryType                 String?
  itemIsExpirationDateRequired            Boolean?
  itemHasExpirationDate                   Boolean?
  itemIsSellByDateRequired                Boolean?
  itemHasSellByDate                       Boolean?
  itemIsUseByDateRequired                 Boolean?
  itemHasUseByDate                        Boolean?
  itemQuantityType                        String?
  itemDefaultLabTestingState              String?
  itemUnitOfMeasureName                   String?
  itemApprovalStatus                      String?
  itemApprovalStatusDateTime              String?
  itemStrainId                            Int?
  itemStrainName                          String?
  itemItemBrandId                         Int?
  itemItemBrandName                       String?
  itemAdministrationMethod                String?
  itemUnitCbdPercent                      String?
  itemUnitCbdContent                      String?
  itemUnitCbdContentUnitOfMeasureName     String?
  itemUnitCbdContentDose                  String?
  itemUnitCbdContentDoseUnitOfMeasureName String?
  itemUnitThcPercent                      String?
  itemUnitThcContent                      String?
  itemUnitThcContentUnitOfMeasureName     String?
  itemUnitThcContentDose                  String?
  itemUnitThcContentDoseUnitOfMeasureId   String?
  itemUnitThcContentDoseUnitOfMeasureName String?
  itemUnitVolume                          Float?
  itemUnitVolumeUnitOfMeasureName         String?
  itemUnitWeight                          Float?
  itemUnitWeightUnitOfMeasureName         String?
  itemServingSize                         String?
  itemNumberOfDoses                       String?
  itemUnitQuantity                        String?
  itemUnitQuantityUnitOfMeasureName       String?
